                    <div class="row">
                      <div class="col-md-4">
                        <input class="form-control" type="text" id="searchDate" placeholder="Search by Date (dd)">
                      </div>
                      <div class="col-md-4">
                        <input class="form-control" type="text" id="searchMonth" placeholder="Search by Month (mm)">
                      </div>
                      <div class="col-md-4">
                        <input class="form-control" type="text" id="searchYear" placeholder="Search by Year (yyyy)">
                      </div>
                    </div>
                  </div>
                  <div class="card-body">
                    <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                      <thead class="thead-dark">
                        <tr>
                          <th>TT</th>
                          <th>Ngày thi đấu</th>
                          <th>Đội 1</th>
                          <th>Điểm đội 1</th>
                          <th>Đội 2</th>
                          <th>Điểm đội 2</th>
                          <th>Sân vận động</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i=0; i < result.length; i++) { %>
                          <tr>
                            <td>
                              <%= i+1 %>
                            </td>
                            <td>
                              <%= result[i]['NGÀY THI ĐẤU'] %>
                            </td>
                            <td>
                              <%= result[i]['ĐỘI 1'] %>
                            </td>
                            <td>
                              <%= result[i]['ĐIỂM ĐỘI 1'] %>
                            </td>
                            <td>
                              <%= result[i]['ĐỘI 2'] %>
                            </td>
                            <td>
                              <%= result[i]['ĐIỂM ĐỘI 2'] %>
                            </td>
                            <td>
                              <%= result[i]['SÂN VẬN ĐỘNG'] %>
                            </td>
                          </tr>
                          <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </main>
