  <script>
    // JavaScript for filtering the table based on user input
    document.addEventListener('DOMContentLoaded', function () {
      document.getElementById('searchDate').addEventListener('input', filterTable);
      document.getElementById('searchMonth').addEventListener('input', filterTable);
      document.getElementById('searchYear').addEventListener('input', filterTable);
    });

    function filterTable() {
      var inputDate = document.getElementById('searchDate').value;
      var inputMonth = document.getElementById('searchMonth').value;
      var inputYear = document.getElementById('searchYear').value;

      var tableRows = document.querySelectorAll('#bootstrap-data-table-export tbody tr');

      tableRows.forEach(function (row) {
        var rowData = row.querySelector('td:nth-child(2)').innerText.split('-'); // Assuming the date is in the second column
        var rowDate = rowData[0];
        var rowMonth = rowData[1];
        var rowYear = rowData[2];

        var dateMatch = rowDate.includes(inputDate);
        var monthMatch = rowMonth.includes(inputMonth);
        var yearMatch = rowYear.includes(inputYear);

        if (dateMatch && monthMatch && yearMatch) {
          row.style.display = '';
        } else {
          row.style.display = 'none';
        }
      });
    }
  </script>
